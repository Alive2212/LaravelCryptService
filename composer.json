{
    "name": "alive2212/laravel-crypt-service",
    "description": "This is laravel crypt service for create rsa & .. key to crypt and decrypt",
    "license": "MIT",
    "authors": [
        {
            "name": "Babak Nodoust",
            "email": "alive2212@yahoo.com",
            "homepage": "http://babakn.com"
        }
    ],
    "homepage": "https://github.com/alive2212/laravelcryptservice",
    "keywords": ["Laravel", "LaravelCryptService"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Alive2212\\LaravelCryptService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alive2212\\LaravelCryptService\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alive2212\\LaravelCryptService\\LaravelCryptServiceServiceProvider"
            ],
            "aliases": {
                "LaravelCryptService": "Alive2212\\LaravelCryptService\\Facades\\LaravelCryptService"
            }
        }
    }
}